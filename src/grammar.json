{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "tree_sitter_seq",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_record"
      }
    },
    "_record": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "fasta_record"
        },
        {
          "type": "SYMBOL",
          "name": "fastq_record"
        }
      ]
    },
    "fasta_record": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "fasta_header"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "fasta_sequence_line"
          }
        }
      ]
    },
    "fasta_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ">"
              },
              {
                "type": "PATTERN",
                "value": "[^\\r\\n]*"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "fasta_sequence_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[A-Za-z]+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "fastq_record": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "fastq_header"
        },
        {
          "type": "SYMBOL",
          "name": "fastq_sequence_line"
        },
        {
          "type": "SYMBOL",
          "name": "fastq_plus_line"
        },
        {
          "type": "SYMBOL",
          "name": "fastq_quality_line"
        }
      ]
    },
    "fastq_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "@"
              },
              {
                "type": "PATTERN",
                "value": "[^\\r\\n]*"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "fastq_sequence_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[A-Za-z]+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "fastq_plus_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "+"
              },
              {
                "type": "PATTERN",
                "value": "[^\\r\\n]*"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "fastq_quality_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "PATTERN",
            "value": "[\\x21-\\x7E]+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "eol"
        }
      ]
    },
    "eol": {
      "type": "PATTERN",
      "value": "\\r?\\n"
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}